  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@discord/embedded-app-sdk": "^1.2.0",
    "zustand": "^4.4.7",
    "express": "^4.18.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/express": "^4.17.21",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "tsx": "^4.7.0",
    "concurrently": "^8.2.2"
  }
